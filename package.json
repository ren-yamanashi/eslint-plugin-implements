{
  "name": "eslint-plugin-type-implements-interface",
  "version": "0.1.0",
  "description": "An ESLint plugin that checks if TypeScript type aliases properly implement interfaces specified in JSDoc `@implements` tags.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./ts-server-plugin": {
      "types": "./dist/ts-server-plugin.d.ts",
      "require": {
        "types": "./dist/ts-server-plugin.d.cts",
        "default": "./dist/ts-server-plugin.cjs"
      },
      "import": {
        "types": "./dist/ts-server-plugin.d.ts",
        "default": "./dist/ts-server-plugin.js"
      }
    }
  },
  "scripts": {
    "build": "tsdown --config tsdown.config.js",
    "test": "vitest --run",
    "lint": "eslint --fix --config eslint.config.js",
    "fmt": "prettier --write \"**/*.{js,json,ts}\" --ignore-path .gitignore",
    "typecheck": "tsc --noEmit",
    "pack": "pnpm run build && npm pack"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/rule-tester": "^8.33.1",
    "eslint": "9.28.0",
    "prettier": "4.0.0-alpha.12",
    "tsdown": "^0.12.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.2"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.33.1",
    "@typescript-eslint/utils": "^8.33.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "homepage": "https://github.com/ren-yamanashi/eslint-plugin-type-implements-interface#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-yamanashi/eslint-plugin-type-implements-interface.git"
  },
  "author": {
    "name": "ren-yamanashi"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
