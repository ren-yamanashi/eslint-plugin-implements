{
  "name": "eslint-plugin-type-implements-interface",
  "version": "0.1.0",
  "description": "An ESLint plugin that checks if TypeScript type aliases properly implement interfaces specified in JSDoc `@implements` tags.",
  "type": "module",
  "exports": {
    "./ts-plugin": {
      "types": "./packages/ts-server-plugin/index.d.ts",
      "require": {
        "types": "./packages/ts-server-plugin/dist/index.d.cts",
        "default": "./packages/ts-server-plugin/dist/index.cjs"
      },
      "import": {
        "types": "./packages/ts-server-plugin/dist/index.d.ts",
        "default": "./packages/ts-server-plugin/dist/index.js"
      }
    },
    "./eslint-plugin": {
      "types": "./packages/eslint-plugin/index.d.ts",
      "require": {
        "types": "./packages/eslint-plugin/dist/index.d.cts",
        "default": "./packages/eslint-plugin/dist/index.cjs"
      },
      "import": {
        "types": "./packages/eslint-plugin/dist/index.d.ts",
        "default": "./packages/eslint-plugin/dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "fmt": "nx run-many --target=fmt --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "test": "nx run-many --target=test --all"
  },
  "devDependencies": {
    "@eslint/js": "catalog:lint",
    "@types/node": "catalog:others",
    "eslint": "catalog:lint",
    "prettier": "catalog:fmt",
    "tsdown": "catalog:build",
    "typescript": "catalog:core",
    "typescript-eslint": "catalog:lint",
    "vitest": "catalog:test",
    "nx": "catalog:others"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript",
    "jsdoc",
    "implements"
  ],
  "homepage": "https://github.com/ren-yamanashi/eslint-plugin-type-implements-interface#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-yamanashi/eslint-plugin-type-implements-interface"
  },
  "author": {
    "name": "ren-yamanashi"
  },
  "license": "MIT"
}
